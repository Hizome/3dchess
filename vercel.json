        }
      ]
    },
    {
      "source": "/models/(.*)\\.(glb|ktx2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/vintage/(.*)\\.(glb|ktx2)",
      "headers": [
        {
          "key": "Cache-Control",
